{
	"env": {
		"es2021": true,
		"node": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 2022,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"extends": [
		"plugin:json/recommended",
		"eslint-config-prettier",
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react-hooks/recommended"
	],
	"plugins": ["json", "eslint-plugin-prettier", "@typescript-eslint", "react"],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn"
	}
}

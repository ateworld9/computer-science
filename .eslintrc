{
  "extends": ["plugin:json/recommended", "eslint-config-prettier"],
  "plugins": ["json", "eslint-plugin-prettier"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
